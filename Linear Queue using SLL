______________________________________________source code___________________________________________

#include<stdio.h>
#include<stdlib.h>
 struct node{
    int data;
    struct node *next;
 }*temp,*new;
 struct node*rear=NULL;
 struct node*front=NULL;
void enqueue()
{
   int value;
   new=(struct node*)malloc(sizeof(struct node));
   printf("enter value: ");
   scanf("%d",&value);
   new->data = value;
   new->next=NULL;
   if(rear==NULL && front==NULL)
   {
      rear=new;
      front=new;
   }
   else{
      rear->next=new;
      rear=new;
   }
   
}
void dequeue()
{
   temp=front;
   printf("popped element: %d\n",temp->data);
   front=front->next;
   temp->next=NULL;
}
void display()
{ printf("Queue elements are:");
   temp=front;
   while(temp!=NULL)
   {
      printf("%d ",temp->data);
      temp=temp->next;
   }
}
void main()
{
int ch=0;
 printf("\n1-enqueue 2-dequeue 3-display\n");
while(ch<4)
{
   printf("enter choice: ");
   scanf("%d",&ch);
   switch(ch)
   {
      case 1:
             enqueue();
             break;
      case 2:
             dequeue();
             break;
      case 3:
             display();
             break;
   }
}
}

_______________________________________________________________output__________________________________________________________________________________________________________
1)
