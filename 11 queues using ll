#include<stdio.h>
#include<stdlib.h>
struct node{
    int data;
    struct node* next;
}*new , *temp,*rear=NULL,*front=NULL;
void enqueue()
{
    int value;
    printf("enter the value of node:\n ");
    scanf("%d",&value);
    new = (struct node*)malloc(sizeof(struct node));
    new->data = value;
    new->next = NULL;
    if(rear==NULL && front==NULL)
    {
        rear=new;
        front=new;
    }
    else
    {
       rear->next = new;
       rear = new;
    }
}
void dequeue()
{
    temp =front;
    printf("popped element is %d",temp->data);
    front = front->next;
    temp->next= NULL;
}
void display()
{
    temp=front;
    while(temp!=NULL)
    {
    printf("%d ",temp->data);
    temp =temp->next;
    }
}
void main()
{
    int ch=0;
    while(ch<4)
    {
        printf("enter che:");
        scanf("%d",&ch);
        switch(ch)
        {
            case 1:
                enqueue();
                break;
            case 2:
                dequeue();
                break;
            case 3:
                display();
                break;
        }
    }
}
