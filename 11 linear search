#include<stdio.h>
void main()
{
    int a[20],n,x,i,flag=0;
    printf("enter no of elements");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter ele to search");
    scanf("%d",&x);
    for(int i=0;i<n;i++)
    {
        if(a[i]==x)
        {
            flag=1;
            break;
        }
    }
    if(flag==1)
    {
        printf("found ");
    }
    else
    {
        printf("not found ");
    }
    
}
