____________________________________________________________Insertion Sort___________________________________________________________________________________________________
#include<stdio.h>
void insertion(int a[],int n)
{ int i,temp,j;
for(i=1;i<n;i++)
{
    temp=a[i];
    j=i-1;
    while(j>=0 && a[j]>temp)
    {
        a[j+1]=a[j];
        j=j-1;
    }
    a[j+1]=temp;
}
for(i=0;i<n;i++)
{
    printf("%d ",a[i]);
   
}
}
void main()
{
int a[10],n,i;
printf("enter no of elements\n");
scanf("%d",&n);
for(i=0;i<n;i++)
{
    printf("enter elements: ");
    scanf("%d",&a[i]);
}
insertion(a,n);
}
______________________________________output___________________________________________________
